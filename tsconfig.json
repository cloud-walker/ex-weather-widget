{
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"noEmit": true,
		"module": "ES2022",
		"moduleResolution": "bundler",
		"skipLibCheck": true,
		"target": "ESNext",
		"paths": {
			"#/*": ["./src/*"],
			"#/panda/*": ["./styled-system/*"]
		}
	},
	"include": ["src", "styled-system"]
}
